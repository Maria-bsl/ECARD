<script type="text/javascript">

  function Getlang(e) {
    var pt = jQuery(e).data('pwd');
    if (pt == 'BNK') {
      jQuery.ajax({
        type: 'POST',
        url: '@Url.Action("Update", "Language")',
        data: JSON.stringify({ 'name': e.value }),
        contentType: 'application/json; charset=utf-8',
        dataType: 'json',
        success: function (data) {
          if (data != null) {
            Getcolumnslang();
            location.reload();
          }
        }
      });

    }
    else if (pt == 'Mem') {
      jQuery.ajax({
        type: 'POST',
        url: '@Url.Action("Update", "LangInst")',
        data: JSON.stringify({ 'name': e.value }),
        contentType: 'application/json; charset=utf-8',
        dataType: 'json',
        success: function (data) {
          if (data != null) {
            location.reload(true);
            Getcolumnslang();
          }
        }
      });
    }
    else {
      jQuery.ajax({
        type: 'POST',
