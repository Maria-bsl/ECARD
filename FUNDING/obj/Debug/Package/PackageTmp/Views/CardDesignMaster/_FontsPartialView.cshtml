@model IEnumerable<FUNDING.Models.CardGenerationModule.DataLayer.FontFamily>

<div class="fonts-search bg-white">
    <div class="heading-text pb-2 pt-2">
        <h3 class="">Card Templates</h3>
    </div>
    <div class="form-group pb-2 search-field">
        <input id="search-font" class="form-control" type="text" name="searchFont" placeholder="Search a font">
        <span class="search-icon-wrapper">
            <i class="fas fa-search"></i>
        </span>
    </div>
</div>

<ul class="list-unstyled">
    @foreach (var item in Model)
    {
        <li>
            <a class="dropdown-item py-2" href="#" style="font-family: '@item.FontName' !important;">@item.FontName</a>
        </li>
    }
</ul>
